[borgbackup]
client ansible_host=127.0.0.1 ansible_port=$(vagrant port client --guest 22) ansible_private_key_file=.vagrant/machines/client/virtualbox/private_key
backup ansible_host=127.0.0.1 ansible_port=$(vagrant port backup --guest 22) ansible_private_key_file=.vagrant/machines/backup/virtualbox/private_key
